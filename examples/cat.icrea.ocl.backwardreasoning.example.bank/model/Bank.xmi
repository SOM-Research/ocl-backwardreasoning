<?xml version="1.0" encoding="ASCII"?>
<bank:Bank xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bank="http://www.example.com/bank"
    xsi:schemaLocation="http://www.example.com/bank bank.ecore">
  <managers
      clients="//@clients.0"
      name="manager1"/>
  <accounts
      owners="//@clients.0"
      credit="100.0"
      overdraft="300.0"/>
  <accounts
      owners="//@clients.1"
      credit="200.0"
      overdraft="300.0"/>
  <clients manager="//@managers.0"
      accounts="//@accounts.0"
      name="client1"
      capacity="2"/>
  <clients accounts="//@accounts.1"
      name="client2"
      capacity="2"/>
</bank:Bank>
